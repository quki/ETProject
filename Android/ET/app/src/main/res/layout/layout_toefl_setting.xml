<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/number_one"
    android:orientation="vertical"
    android:paddingLeft="35dp"
    android:paddingRight="35dp"
    android:weightSum="1">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:orientation="horizontal">


        <ImageButton
            android:id="@+id/toeic_setting"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/rounded_button_setting"
            android:paddingBottom="20dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="20dp"
            android:scaleType="fitCenter"
            android:src="@drawable/setting"
            android:layout_marginLeft="20dp"

            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="10dp">

        <Button
            android:id="@+id/toeic"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:width="200dp"
            android:height="35dp"
            android:background="@color/number_four"
            android:paddingLeft="30dp"
            android:paddingRight="30dp"
            android:text="T O E F L"
            android:textColor="@color/black"
            android:textSize="22dp"

            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="2dp">

        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:width="200dp"
            android:background="@color/number_six"
            android:minHeight="10dp"
            android:paddingLeft="30dp"
            android:paddingRight="30dp"
            android:textColor="@color/black"
            android:textSize="15dp"

            />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/number_two"
        android:paddingTop="5dp"
        android:minHeight="40dp"
        >



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:text = "Reading"
            android:textSize ="16dp"
            android:textColor="@color/black"
            />
        <EditText
            android:layout_marginLeft="50dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@color/number_three"
            android:height="40dp"
            android:width="40dp"

            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginLeft="5dp"
            android:text = "min"
            android:textSize ="15dp"
            android:textColor="@color/black"
            />
        <EditText
            android:layout_marginLeft="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@color/number_three"
            android:height="40dp"
            android:width="40dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginLeft="5dp"
            android:text = "sec"
            android:textSize ="15dp"
            android:textColor="@color/black"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/number_two"
        android:paddingTop="5dp"
        android:minHeight="40dp"

        >



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:text = "Speaking"
            android:textSize ="16dp"
            android:textColor="@color/black"
            />
        <EditText
            android:layout_marginLeft="44dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@color/number_three"
            android:height="40dp"
            android:width="40dp"

            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginLeft="5dp"
            android:text = "min"
            android:textSize ="15dp"
            android:textColor="@color/black"
            />
        <EditText
            android:layout_marginLeft="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@color/number_three"
            android:height="40dp"
            android:width="40dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginLeft="5dp"
            android:text = "sec"
            android:textSize ="15dp"
            android:textColor="@color/black"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/number_two"
        android:paddingTop="5dp"
        android:minHeight="35dp"
        android:gravity="center"
        >



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"

            android:text = "Writing"
            android:textSize ="18dp"
            android:textColor="@color/black"
            />

    </LinearLayout>




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:orientation="horizontal">


        <Button
            android:id="@+id/toefl_total"
            android:layout_width="200dp"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:background="@color/number_two"
            android:scaleType="fitCenter"
            android:text="     통합형"
            android:textSize="20dp"
            android:layout_marginRight="2.5dp"

            />


        <Button
            android:id="@+id/toefl_indipendent"
            android:layout_width="200dp"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:background="@color/number_two"
            android:scaleType="fitCenter"
            android:text="       독립형"
            android:textAlignment="viewStart"
            android:textSize="20dp"
            android:layout_marginRight="2.5dp"

            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:orientation="horizontal">


        <ImageButton
            android:id="@+id/toeic_start2"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_gravity="center"
            android:background="@drawable/rounded_button_start"
            android:paddingBottom="20dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="20dp"
            android:scaleType="fitCenter"
            android:src="@drawable/back"
            android:layout_marginRight="20dp"

            />


        <ImageButton
            android:id="@+id/toeic_save"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:background="@drawable/rounded_button_setting"
            android:paddingBottom="20dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="20dp"
            android:scaleType="fitCenter"
            android:src="@drawable/save"
            android:layout_marginLeft="20dp"

            />

    </LinearLayout>


</LinearLayout>